	<queryString language="MOCA">
		<![CDATA[if (@arecod = '' or @arecod = ' ')
{
    publish data
     where whereclause = ' and 1 = 1'
}
else
{
    publish data
     where whereclause = ' and aremst.arecod = ' || '''' || @arecod || ''''
}
|
[select dscmst.lngdsc as 'area',
        sto_zone.sto_zone_cod as 'Storage Zone',
        iv.stoloc as 'Location',
        iv.lodnum as 'LPN',
        iv.lotnum,
        iv.inv_attr_str8 as 'PAB Number',
        prtdsc.short_dsc as 'Item Description',
        rcvlin.idnqty as 'qty from ASN',
        untqty as 'current qty'
   from inventory_view iv,
        rcvlin,
        prtdsc,
        locmst,
        loc_typ,
        sto_zone,
        aremst,
        dscmst
  where iv.rcvkey = rcvlin.rcvkey
    and prtdsc.colval = iv.prtnum || '|' || iv.prt_client_id || '|' || iv.wh_id
    and prtdsc.locale_id = 'US_ENGLISH'
    and locmst.stoloc = iv.stoloc
    and locmst.loc_typ_id = loc_typ.loc_typ_id
    and loc_typ.fwiflg = '1'
    and sto_zone.sto_zone_id = locmst.sto_zone_id
    and locmst.arecod = aremst.arecod
    and dscmst.colnam = 'arecod|wh_id'
    and dscmst.colval = aremst.arecod || '|' || iv.wh_id @whereclause:raw
  group by dscmst.lngdsc,
        sto_zone.sto_zone_cod,
        iv.stoloc,
        iv.lodnum,
        iv.lotnum,
        iv.inv_attr_str8,
        prtdsc.short_dsc,
        rcvlin.idnqty,
        iv.untqty
  order by dscmst.lngdsc,
        sto_zone.sto_zone_cod,
        iv.stoloc]]]>
	</queryString>
